# Copy to .env and replace the placeholder values with your Slack tokens
SLACK_BOT_TOKEN=your-bot-token
SLACK_APP_TOKEN=your-app-level-token
# Optional comma separated plugin package overrides / enable list
# PLUGIN_PACKAGES=plugins
# ENABLED_PLUGINS=
# Persistent storage location (leave blank to fall back to in-memory)
# DATABASE_URL=sqlite+aiosqlite:///./bot.db
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/zatech_bot

# Firebase Auth
FIREBASE_CREDENTIALS_JSON = {your-firebase-credentials-json}

# Production environment flag
ENVIRONMENT=development/production
